<%= simple_form_for(@contest) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :begin_at %>
    <%= f.input :end_at %>
    <%= f.input :allow_edit %>
    <%= f.input :rules_url %>
    <%= f.input :technical_email %>
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
