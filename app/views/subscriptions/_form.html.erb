<%= semantic_form_for @subscription, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.semantic_errors %>
  <%= f.inputs do %>
    <%= f.input :id_card %>
    <%= f.input :email %>
    <%= f.input :event_id %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit %>
  <% end %>
<% end %>
